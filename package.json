{
  "version": "0.0.5",
  "license": "MIT",
  "name": "@azvaliev/kysely-sqlite-local",
  "type": "commonjs",
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "find . -name '*.d.ts' -not -path './node_modules/*' -type f -exec rm -rf {} + && tsc",
    "publish": "yarn build && yarn npm publish --access public"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.1.0",
    "kysely-codegen": "^0.18.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "env-paths": "^3.0.0",
    "kysely": "^0.28.3"
  },
  "main": "index.js",
  "types": "types.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "bin": {
    "kysely-sqlite-codegen": "./codegen.js"
  }
}
